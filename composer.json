{
    "name": "konopden/bitrix-np-import",
    "description": "Import and updatig data from \"Nova Poshta\" for sale module.",
    "keywords": ["bitrix"],
    "license": "MIT",
    "type": "library",
    "require": {
        "pear/http_request2": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BitrixNovaPoshta\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
